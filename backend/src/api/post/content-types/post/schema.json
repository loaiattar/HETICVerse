{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "blocks",
      "required": true
    },
    "vote": {
      "type": "integer",
      "default": 0
    },
    "type": {
      "type": "enumeration",
      "enum": [
        "link",
        "text",
        "image"
      ]
    },
    "image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "URL": {
      "type": "string"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment"
    },
    "community": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::community.community",
      "inversedBy": "post"
    },
    "users_permissions_user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "post"
    },
    "comment": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::comment.comment",
      "mappedBy": "post"
    }
  }
}
