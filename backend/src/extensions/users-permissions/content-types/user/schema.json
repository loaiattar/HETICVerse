{
  "kind": "collectionType",
  "collectionName": "up_users",
  "info": {
    "name": "user",
    "description": "",
    "singularName": "user",
    "pluralName": "users",
    "displayName": "User"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3,
      "unique": true,
      "configurable": false,
      "required": true
    },
    "email": {
      "type": "email",
      "minLength": 6,
      "configurable": false,
      "required": true
    },
    "provider": {
      "type": "string",
      "configurable": false
    },
    "password": {
      "type": "password",
      "minLength": 6,
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "resetPasswordToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmationToken": {
      "type": "string",
      "configurable": false,
      "private": true,
      "searchable": false
    },
    "confirmed": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "blocked": {
      "type": "boolean",
      "default": false,
      "configurable": false
    },
    "role": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.role",
      "inversedBy": "users",
      "configurable": false
    },
    "profilePicture": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": ["images"]
    },
    "bio": {
      "type": "text"
    },
    "favoriteColor": {
      "type": "string"
    },
    "birthday": {
      "type": "date"
    },
    "karma": {
      "type": "integer",
      "default": 0
    },
    "badges": {
      "type": "json"
    },
    "following": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "followers"
    },
    "followers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "following"
    },
    "posts": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::post.post",
      "mappedBy": "author"
    },
    "comments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::comment.comment",
      "mappedBy": "author"
    },
    "votes": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::vote.vote",
      "mappedBy": "users_permissions_user"
    },
    "sub_hetic_verses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::sub-hetic-verse.sub-hetic-verse",
      "mappedBy": "author"
    },
    "conversations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::chat.conversation",
      "mappedBy": "participants"
    },
     "ownedCommunities": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::community.community",
      "mappedBy": "owner"
    },
    "communityMemberships": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::community-member.community-member",
      "mappedBy": "user"
    }
  }
}